<?xml version="1.0" encoding="utf-8" ?>
<odoo>
        <!-- mail template for birthday wish -->
        <record id="mail_template_birthday_wish" model="mail.template">
            <field name="name">Contacts: Birthday Wish</field>
            <field name="model_id" ref="partner_contact_birthdate.model_res_partner" />
            <field name="subject">Birthday Wish</field>
            <field name="body_html" type="html">
                <div>
                    <p>
                       Happy Birthday!
                    </p>
                </div>
            </field>
        </record>
</odoo>
